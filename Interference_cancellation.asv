function rx_data = Interference_cancellation(Y,hs,hj,cp_len,N_subcarriers,occupied_subcarriers)
%INTERFERENCE_CANCELLATION Projects the received signal onto a different subspace
%and obtains the projected received signal 
 %alpha = hj(1) / hj(2);
 proj_vec = [hj(2);-hj(1)];
 y_proj = proj_vec' * Y;
 %rx_dec_data = y_proj/(hj(2)*hs(1)-hj(1)*hj(2));
 rx_no_cp = y_proj(cp_len+1:end);
 rx_fft = fft(rx_no_cp, N_subcarriers);
 rx_data = rx_fft(1:occupied_subcarriers);
end

